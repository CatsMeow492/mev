{
  "name": "mev-engine-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:staging": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=staging next build",
    "build:production": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "clean": "rm -rf .next && rm -rf out",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "postbuild": "next-sitemap",
    "analyze": "npm run build:analyze && npx @next/bundle-analyzer .next/server/pages",
    "performance": "npm run build && npx unlighthouse --site http://localhost:3000"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "socket.io-client": "^4.7.0",
    "axios": "^1.6.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@playwright/test": "^1.40.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "@next/bundle-analyzer": "^14.0.0",
    "webpack-bundle-analyzer": "^4.10.0",
    "next-sitemap": "^4.2.3",
    "cross-env": "^7.0.3",
    "vercel": "^32.5.0",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.0",
    "unlighthouse": "^0.10.0",
    "lighthouse": "^11.3.0"
  }
}